{
  "multipart": [
    {
      "apply": { "model": "lacrimis:block/duct/gated_node" }
    },
    {
      "when": { "down": true },
      "apply": { "model": "lacrimis:block/duct/side" }
    },
    {
      "when": { "up": true },
      "apply": { "model": "lacrimis:block/duct/side", "x": 180 }
    },
    {
      "when": { "north": true },
      "apply": { "model": "lacrimis:block/duct/side", "x": 90, "y": 180 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "lacrimis:block/duct/side", "x": 90 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "lacrimis:block/duct/side", "x": 90, "y": 90 }
    },
    {
      "when": { "east": true },
      "apply": { "model": "lacrimis:block/duct/side", "x": 90, "y": 270 }
    }
  ]
}